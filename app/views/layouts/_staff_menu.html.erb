<% if current_staff.admin == true || current_staff.super_admin == true %>
  <% if session[:department_dashboard] == true %>
    <%= render 'layouts/departments_menu' %>
  <% elsif session[:employee_profile] == true %>
    <%= render 'layouts/employee_menu' %>
  <% elsif session[:super_admin_profile] == true %>
    <%= render 'layouts/super_admin_menu' %>
  <% end %>
<% end %>